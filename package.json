{
  "name": "ecommerce-image-extractor",
  "description": "电商图片抓取器 - 智能识别电商网站商品图片，支持批量下载和分类管理",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.10.1",
    "@wxt-dev/module-svelte": "^2.0.3",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "lucide-svelte": "^0.555.0"
  }
}
